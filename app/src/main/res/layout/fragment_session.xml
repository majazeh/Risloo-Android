<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/White"
    android:fillViewport="true"
    android:scrollbarSize="0dp"
    android:overScrollMode="always"
    android:layoutDirection="rtl"
    tools:context=".Views.Fragments.Show.SessionFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/_12sdp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/container_constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/info_constraintLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12sdp"
                android:layout_marginStart="@dimen/_12sdp"
                android:paddingBottom="@dimen/_12sdp"
                android:background="@drawable/draw_2sdp_solid_transparent_border_1sdp_gray200"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/serial_imageView"
                    android:layout_width="@dimen/_16sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:layout_marginEnd="@dimen/_12sdp"
                    android:contentDescription="@string/AppImageContent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/ic_clipboard_light"
                    app:layout_constraintBottom_toBottomOf="@id/serial_textView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@id/serial_textView"
                    app:tint="@color/Blue700" />

                <TextView
                    android:id="@+id/serial_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_6sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:gravity="start"
                    android:textSize="@dimen/_10ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Blue700"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_bold"
                    app:layout_constraintEnd_toStartOf="@id/serial_imageView"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="سریال" />

                <ImageView
                    android:id="@+id/date_imageView"
                    android:layout_width="@dimen/_14sdp"
                    android:layout_height="@dimen/_14sdp"
                    android:layout_marginStart="@dimen/_12sdp"
                    android:contentDescription="@string/AppImageContent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/ic_calendar_alt_light"
                    app:layout_constraintBottom_toBottomOf="@id/date_textView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/date_textView"
                    app:tint="@color/Gray500" />

                <TextView
                    android:id="@+id/date_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6sdp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:gravity="start"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray500"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_regular"
                    app:layout_constraintEnd_toEndOf="@id/serial_imageView"
                    app:layout_constraintStart_toEndOf="@id/date_imageView"
                    app:layout_constraintTop_toBottomOf="@id/serial_textView"
                    tools:text="تاریخ جلسه" />

                <ImageView
                    android:id="@+id/duration_imageView"
                    android:layout_width="@dimen/_14sdp"
                    android:layout_height="@dimen/_14sdp"
                    android:contentDescription="@string/AppImageContent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/ic_clock_light"
                    app:layout_constraintBottom_toBottomOf="@id/duration_textView"
                    app:layout_constraintStart_toStartOf="@id/date_imageView"
                    app:layout_constraintTop_toTopOf="@id/duration_textView"
                    app:tint="@color/Gray500" />

                <TextView
                    android:id="@+id/duration_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:gravity="start"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray500"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_regular"
                    app:layout_constraintEnd_toEndOf="@id/serial_imageView"
                    app:layout_constraintStart_toStartOf="@id/date_textView"
                    app:layout_constraintTop_toBottomOf="@id/date_textView"
                    tools:text="مدت‌زمان جلسه" />

                <TextView
                    android:id="@+id/status_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:gravity="start"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray500"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_regular"
                    app:layout_constraintEnd_toEndOf="@id/serial_imageView"
                    app:layout_constraintStart_toStartOf="@id/duration_imageView"
                    app:layout_constraintTop_toBottomOf="@id/duration_textView"
                    tools:text="وضعیت" />

                <include
                    android:id="@+id/reports_textView"
                    layout="@layout/component_main_button_rect_mid"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_28sdp"
                    android:layout_marginTop="@dimen/_8sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@id/status_textView"
                    app:layout_constraintTop_toBottomOf="@id/status_textView" />

                <include
                    android:id="@+id/edit_imageView"
                    layout="@layout/component_main_button_oval_mid"
                    android:layout_width="@dimen/_28sdp"
                    android:layout_height="@dimen/_28sdp"
                    android:layout_marginTop="@dimen/_8sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@id/serial_imageView"
                    app:layout_constraintTop_toBottomOf="@id/status_textView" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/info2_constraintLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12sdp"
                android:padding="@dimen/_12sdp"
                android:background="@drawable/draw_2sdp_solid_transparent_border_1sdp_gray200"
                app:layout_constraintEnd_toEndOf="@id/info_constraintLayout"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/info_constraintLayout">

                <TextView
                    android:id="@+id/session_type_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/SessionFragmentSessionType"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/session_type_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/session_type_hint_textView"
                    tools:text="حضوری" />

                <View
                    android:id="@+id/session_type_view"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/Gray200"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/session_type_textView" />

                <TextView
                    android:id="@+id/reference_count_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="@string/SessionFragmentReferenceCount"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/session_type_view" />

                <TextView
                    android:id="@+id/reference_count_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/reference_count_hint_textView"
                    tools:text="10" />

                <View
                    android:id="@+id/reference_count_view"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/Gray200"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/reference_count_textView" />

                <TextView
                    android:id="@+id/payment_type_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="@string/SessionFragmentPaymentType"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/reference_count_view" />

                <TextView
                    android:id="@+id/payment_type_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/payment_type_hint_textView"
                    tools:text="آنلاین" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/info3_constraintLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12sdp"
                android:padding="@dimen/_12sdp"
                android:background="@drawable/draw_2sdp_solid_transparent_border_1sdp_gray200"
                app:layout_constraintEnd_toEndOf="@id/info_constraintLayout"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/info2_constraintLayout">

                <TextView
                    android:id="@+id/selection_type_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/SessionFragmentSelectionType"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/selection_type_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/selection_type_hint_textView"
                    tools:text="-" />

                <View
                    android:id="@+id/selection_type_view"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/Gray200"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/selection_type_textView" />

                <TextView
                    android:id="@+id/reference_type_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="@string/SessionFragmentReferenceType"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/selection_type_view" />

                <TextView
                    android:id="@+id/reference_type_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/reference_type_hint_textView"
                    tools:text="-" />

                <View
                    android:id="@+id/reference_type_view"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/Gray200"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/reference_type_textView" />

                <TextView
                    android:id="@+id/start_time_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="@string/SessionFragmentStartTime"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/reference_type_view" />

                <TextView
                    android:id="@+id/start_time_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/start_time_hint_textView"
                    tools:text="-" />

                <View
                    android:id="@+id/start_time_view"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/Gray200"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/start_time_textView" />

                <TextView
                    android:id="@+id/end_time_hint_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="@string/SessionFragmentEndTime"
                    android:textSize="@dimen/_8ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray400"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/start_time_view" />

                <TextView
                    android:id="@+id/end_time_textView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/_9ssp"
                    android:textStyle="normal"
                    android:textColor="@color/Gray600"
                    android:textAlignment="gravity"
                    android:fontFamily="@font/dana_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/end_time_hint_textView"
                    tools:text="-" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <include
                android:id="@+id/psychologists_header_includeLayout"
                layout="@layout/component_main_index_header"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_18sdp"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/info3_constraintLayout" />

            <include
                android:id="@+id/psychologists_shimmerLayout"
                layout="@layout/component_shimmer_psychology"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/psychologists_header_includeLayout"
                tools:visibility="gone" />

            <include
                android:id="@+id/psychologists_singleLayout"
                layout="@layout/component_single_psychology"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/psychologists_header_includeLayout"
                tools:visibility="visible" />

            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/psychologists_barrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="bottom"
                app:constraint_referenced_ids="psychologists_shimmerLayout, psychologists_singleLayout" />

            <include
                android:id="@+id/users_header_includeLayout"
                layout="@layout/component_main_index_header"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_18sdp"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/psychologists_barrier" />

            <include
                android:id="@+id/users_add_imageView"
                layout="@layout/component_main_button_oval_mid"
                android:layout_width="@dimen/_28sdp"
                android:layout_height="@dimen/_28sdp"
                app:layout_constraintBottom_toBottomOf="@id/users_header_includeLayout"
                app:layout_constraintEnd_toEndOf="@id/info_constraintLayout"
                app:layout_constraintTop_toTopOf="@id/users_header_includeLayout" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/users_containerLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12sdp"
                android:padding="@dimen/_1sdp"
                android:background="@drawable/draw_2sdp_solid_transparent_border_1sdp_gray200"
                app:layout_constraintEnd_toEndOf="@id/info_constraintLayout"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/users_add_imageView">

                <include
                    android:id="@+id/users_shimmerLayout"
                    layout="@layout/component_shimmer_index"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:visibility="gone" />

                <include
                    android:id="@+id/users_headerLayout"
                    layout="@layout/header_item_user2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:visibility="visible" />

                <include
                    android:id="@+id/users_singleLayout"
                    layout="@layout/component_single_user2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/users_headerLayout"
                    tools:visibility="visible" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <include
                android:id="@+id/practices_header_includeLayout"
                layout="@layout/component_main_index_header"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_18sdp"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/users_containerLayout" />

            <include
                android:id="@+id/practices_add_imageView"
                layout="@layout/component_main_button_oval_mid"
                android:layout_width="@dimen/_28sdp"
                android:layout_height="@dimen/_28sdp"
                app:layout_constraintBottom_toBottomOf="@id/practices_header_includeLayout"
                app:layout_constraintEnd_toEndOf="@id/info_constraintLayout"
                app:layout_constraintTop_toTopOf="@id/practices_header_includeLayout" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/practices_containerLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12sdp"
                android:padding="@dimen/_1sdp"
                android:background="@drawable/draw_2sdp_solid_transparent_border_1sdp_gray200"
                app:layout_constraintEnd_toEndOf="@id/info_constraintLayout"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/practices_add_imageView">

                <include
                    android:id="@+id/practices_shimmerLayout"
                    layout="@layout/component_shimmer_index"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:visibility="gone" />

                <include
                    android:id="@+id/practices_headerLayout"
                    layout="@layout/header_item_practice"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:visibility="visible" />

                <include
                    android:id="@+id/practices_singleLayout"
                    layout="@layout/component_single_practice"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practices_headerLayout"
                    tools:visibility="visible" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <include
                android:id="@+id/samples_header_includeLayout"
                layout="@layout/component_main_index_header"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_18sdp"
                app:layout_constraintStart_toStartOf="@id/info_constraintLayout"
                app:layout_constraintTop_toBottomOf="@id/practices_containerLayout" />

            <include
